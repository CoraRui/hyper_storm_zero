; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="hyper_storm_zero"
run/main_scene="res://root.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://raw_assets/icon.svg"

[autoload]

app_manager_auto="*res://app_manager/autoloads/app_manager.tscn"
music_player_auto="*res://audio/music/autoloads/music_player/music_player.tscn"
sfx_player_auto="*res://audio/sfx/autoloads/sfx_player.tscn"
debug_auto="*res://debug_auto/debug.tscn"
save_manager_auto="*res://save_manager/autoload/save_manager.tscn"
scene_manager_auto="*res://scene_manager/autoload/scene_manager.tscn"
player_loader_auto="*res://player_loader/autoloads/player_loader/player_loader.tscn"
simple_fx_auto="*res://simple_fx/autoloads/simple_fx.tscn"
screen_fx_auto="*res://screen_fx/autoloads/screen_fx.tscn"
text_box_auto="*res://ui/text_box/autoloads/text_box.tscn"
enemy_loader_auto="*res://enemy_loader/autoloads/enemy_loader.tscn"
score_manager_auto="*res://ui/score_manager/autoload/score_manager.tscn"
simple_drop_auto="*res://simple_drop/autoloads/simple_drop.tscn"
shmup_health_auto="*res://player_loader/nodes/shmup_health/shmup_health.tscn"
block_manager_auto="*res://ui/grid_ui/autoloads/block_manager.tscn"
node_executor_auto="*res://node_command/autoloads/node_executor.tscn"

[display]

window/size/viewport_width=125
window/size/viewport_height=175
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg")

[gui]

theme/custom_font="res://raw_assets/fonts/Teeny Tiny Pixls.otf"

